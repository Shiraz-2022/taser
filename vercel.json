{
  "builds": [
    {
      "src": "./src/index.jsx",
      "use": "@vercel/static"
    },
    {
      "src": "./server/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/.*",
      "dest": "./server/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "./index.html"
    }
  ]
}
